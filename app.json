{
  "expo": {
    "name": "ARES-Site",
    "slug": "ARES-Site",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ARES-Logo-Green.png",
    "scheme": "aressite",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ARES-Logo-Green.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "viewport": "width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no",
      "favicon": "./assets/images/ARES-Logo-Green.png",
      "router": {
        "basename": ".",
        "typedRoutes": true
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ARES-Logo-Green.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}